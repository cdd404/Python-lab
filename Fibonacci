nterms=int(input("Enter N numbers"))
n1,n2=0,1
count=0
if nterms<=0:
    print("please input positive integers")
elif nterms==1:
    print(f"fibanocci sequence of {nterms} is : {nterms}")
else:
    print("Fibonacci sequence:")
    while count < nterms:
        print(n1)  
        nth = n1 + n2  
        n1 = n2  
        n2 = nth 
        count += 1  
